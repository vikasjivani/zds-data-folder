    <input file="1_matt_country.csv" separator="," type="com.axelor.apps.base.db.Country" search="self.importId = :importId" />
    
    <input file="2_matt_state(region).csv" separator="," type="com.axelor.apps.base.db.Region" search="self.importId = :importId" />
    
    <input file="3_matt_counties(territory).csv" separator="," type="com.axelor.apps.base.db.Department" search="self.importId = :importId" />
    
    <input file="4_matt_city(City).csv" separator="," type="com.axelor.apps.base.db.City" search="self.importId = :importId" />
    
    <input file="5_matt_postal_code.csv" separator="," type="com.axelor.apps.base.db.PostalCode" search="self.importId = :importId" />
    
    <input file="6_matt_address.csv" separator="," type="com.axelor.apps.base.db.Address" search="self.importId = :importId" />

    <input file="7_matt_team.csv" separator="," type="com.axelor.team.db.Team">
	<bind to="members" column="members" search="self.code in :members" eval="members.split('\\|') as List"/>
    </input>
    
    <input file="product.csv" separator="," type="com.axelor.apps.base.db.Product" search="self.importId = :importId" />

    
    <input file="email.csv" separator="," type="com.axelor.message.db.EmailAddress" search="self.importId = :importId" />

    <input file="address.csv" separator="," type="com.axelor.apps.base.db.Address" search="self.importId = :importId" />

     <input file="pad.csv" separator="," type="com.axelor.apps.base.db.PartnerAddress" search="self.importId = :importId" >
        <bind column="address_importId" to="address" search="self.importId = :address_importId" update="true">
            <bindingsLinked>false</bindingsLinked>
        </bind>
    </input>

    <input file="customer.csv" separator="," type="com.axelor.apps.base.db.Partner" search="self.importId = :importId">

        <bind column="partnerSeq_importId" to="partnerSeq" search="self.importId = :partnerSeq_importId"  eval="partnerSeq_importId as sequence"  />
            
        <bind column="emailAddress_importId" to="emailAddress" search="self.importId = :emailAddress_importId" />
            
        <bind column="partnerAddress_importId" to="partnerAddressList" search="self.importId in :partnerAddress_importId" eval="partnerAddress_importId.split('\\|') as List" />
        


    </input>
    
    <input file="1_account_type.csv" separator="," type="com.axelor.apps.account.db.AccountType" search="self.importId = :importId" />
    
    <input file="2_account.csv" separator="," type="com.axelor.apps.account.db.Account" search="self.importId = :importId" />
    
</csv-inputs>



